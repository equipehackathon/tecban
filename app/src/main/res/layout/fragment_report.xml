<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv_rd_porc"
        android:layout_width="108dp"
        android:layout_height="110dp"
        android:layout_below="@+id/textView6"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="265dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="37dp"
        android:text="0.0%"
        android:textAlignment="center"
        android:textSize="24sp" />

    <include
        android:id="@+id/header"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        layout="@layout/header_view"/>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="267dp"
        android:layout_height="158dp"
        android:layout_below="@+id/header"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="74dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="70dp"
        android:text="@string/text_reports"
        android:textSize="24sp"
        android:visibility="visible" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="211dp"
        android:layout_height="257dp"
        android:layout_below="@+id/textView2"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="98dp"
        android:layout_marginTop="-58dp"
        android:layout_marginEnd="102dp"
        app:srcCompat="@drawable/solid_circle" />

    <TextView
        android:id="@+id/tv_value"
        android:layout_width="129dp"
        android:layout_height="34dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="139dp"
        android:layout_marginTop="329dp"
        android:layout_marginEnd="141dp"
        android:text="0"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="142dp"
        android:layout_height="31dp"
        android:layout_below="@+id/imageView3"
        android:layout_alignParentStart="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="-4dp"
        android:layout_marginEnd="68dp"
        android:layout_toStartOf="@+id/textView7"
        android:text="Fluxo de Caixa" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="153dp"
        android:layout_height="33dp"
        android:layout_below="@+id/imageView3"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="-3dp"
        android:layout_marginEnd="13dp"
        android:text="Rendimentos 6 meses" />

    <TextView
        android:id="@+id/tv_fc_porc"
        android:layout_width="108dp"
        android:layout_height="110dp"
        android:layout_below="@+id/textView6"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="45dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="257dp"
        android:text="0.0%"
        android:textAlignment="center"
        android:textSize="24sp" />


</RelativeLayout>